{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": ["plugin:prettier/recommended", "eslint:recommended"],
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8,
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      }
  },
  "plugins": ["prettier"],
  "rules": {
      "indent": ["error", 2],
      "linebreak-style": ["error", "unix"],
      "no-console": 0,
      "prettier/prettier": "error",
      "quotes": ["error", "double"],
      "semi": ["error", "always"]
  }
}